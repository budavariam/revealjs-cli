{
  "name": "revealjs-cli",
  "displayName": "revealjs-cli",
  "version": "0.0.7",
  "description": "Show markdown as revealJs presentation",
  "bin": {
    "revealjs-cli": "dist/revealjs-cli.js"
  },
  "main": "./dist/revealjs-cli.js",
  "publisher": "budavariam",
  "repository": {
    "type": "git",
    "url": "https://github.com/budavariam/revealjs-cli"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "keywords": [
    "markdown",
    "reveal js",
    "slide",
    "presentation"
  ],
  "jest": {
    "verbose": true,
    "preset": "ts-jest",
    "testMatch": [
      "**/test/**/*.jest.ts"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "collectCoverageFrom": [
      "**/src/**/*.ts",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ]
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/libs/*"
    ],
    "reporter": [
      "html",
      "lcov"
    ],
    "all": true
  },
  "dependencies": {
    "axios": "^1.7.0",
    "cheerio": "^1.0.0",
    "commander": "^12.0.0",
    "fs-jetpack": "^5.1.0",
    "gray-matter": "^4.0.3",
    "koa": "^2.15.0",
    "koa-ejs": "^4.3.0",
    "koa-logger": "^3.2.1",
    "koa-static": "^5.0.0",
    "koa-static-server": "^1.5.2",
    "markdown-it": "^10.0.0",
    "markdown-it-attrs": "^3.0.3",
    "markdown-it-block-embed": "0.0.3",
    "markdown-it-container": "^2.0.0",
    "markdown-it-github-headings": "^1.1.2",
    "markdown-it-imsize": "^2.0.1",
    "markdown-it-multimd-table": "^3.2.3",
    "markdown-it-regexp": "^0.4.0",
    "markdown-it-span": "^1.0.0",
    "markdown-it-task-lists": "^2.1.1"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.0",
    "@types/koa": "^2.15.0",
    "@types/koa-ejs": "^4.3.0",
    "@types/koa-router": "^7.4.8",
    "@types/koa-static": "^4.0.4",
    "@types/markdown-it": "^10.0.0",
    "@types/marked": "^6.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^20.0.0",
    "@types/webpack": "^5.28.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "mocha": "^10.7.0",
    "mocha-junit-reporter": "^2.2.0",
    "mocha-multi-reporters": "^1.5.1",
    "nyc": "^17.1.0",
    "source-map-explorer": "^2.5.3",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.2.0",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.0",
    "tslint": "^6.1.3",
    "typescript": "^5.6.0",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.0",
    "webpack-node-externals": "^3.0.0"
  },
  "scripts": {
    "analyze": "source-map-explorer 'dist/*.js'",
    "build": "webpack --mode production",
    "postbuild": "node utils/set_execute_permission.js",
    "compile:tsc": "tsc -watch -p ./",
    "build:tsc": "tsc -p ./",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "tslint -t verbose './src/**/*'",
    "webpack": "webpack --mode development",
    "postwebpack": "node utils/set_execute_permission.js",
    "webpack-dev": "webpack --mode development --watch",
    "prenpmpublish": "npm run build && npm version patch ",
    "npmpublish": "npm publish"
  },
  "license": "MIT"
}
