{
  "name": "revealjs-cli-server",
  "displayName": "revealjs-cli-server",
  "version": "0.0.1",
  "description": "Show markdown as revealJs presentation",
  "bin": {
    "revealjs-cli": "dist/revealjs-cli-server.js"
  },
  "main": "./dist/revealjs-cli-server.js",
  "publisher": "budavariam",
  "repository": {
    "type": "git",
    "url": "https://github.com/budavariam/revealjs-cli-server"
  },
  "keywords": [
    "markdown",
    "reveal js",
    "slide",
    "presentation"
  ],
  "jest": {
    "verbose": true,
    "preset": "ts-jest",
    "testMatch": [
      "**/test/**/*.jest.ts"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "collectCoverageFrom": [
      "**/src/**/*.ts",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ]
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/libs/*"
    ],
    "reporter": [
      "html",
      "lcov"
    ],
    "all": true
  },
  "dependencies": {
    "@evilz/markdown-it-attrs": "^3.0.1",
    "@types/koa-static-server": "^1.3.0",
    "axios": "^0.21.0",
    "commander": "^2.20.3",
    "fs-jetpack": "^2.2.2",
    "gray-matter": "^4.0.2",
    "koa": "^2.11.0",
    "koa-ejs": "^4.2.0",
    "koa-logger": "^3.2.1",
    "koa-static-server": "^1.4.0",
    "markdown-it": "^10.0.0",
    "markdown-it-block-embed": "0.0.3",
    "markdown-it-container": "^2.0.0",
    "markdown-it-github-headings": "^1.1.2",
    "markdown-it-imsize": "^2.0.1",
    "markdown-it-multimd-table": "^3.2.3",
    "markdown-it-regexp": "^0.4.0",
    "markdown-it-span": "^1.0.0",
    "markdown-it-task-lists": "^2.1.1",
    "opn": "^6.0.0",
    "os": "^0.1.1"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/jest": "^24.0.18",
    "@types/koa": "^2.0.49",
    "@types/koa-ejs": "^4.2.0",
    "@types/koa-router": "^7.0.42",
    "@types/koa-static": "^4.0.1",
    "@types/markdown-it": "0.0.9",
    "@types/marked": "^0.6.5",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.14.15",
    "@types/opn": "^5.5.0",
    "@types/request": "^2.48.2",
    "@types/showdown": "^1.9.3",
    "@types/webpack": "^4.39.2",
    "@types/yargs": "^13.0.11",
    "jest": "^24.9.0",
    "jest-junit": "^8.0.0",
    "mocha": "^6.2.2",
    "mocha-junit-reporter": "^1.23.1",
    "mocha-multi-reporters": "^1.1.7",
    "nyc": "^14.1.1",
    "request": "^2.88.0",
    "sonarqube-scanner": "^2.5.0",
    "source-map-support": "^0.5.13",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.2.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "^3.6.3",
    "typescript-tslint-plugin": "^0.5.4",
    "vscode": "^1.1.36",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9",
    "webpack-node-externals": "^1.7.2"
  },
  "scripts": {
    "build": "webpack --mode production && chmod +x dist/*",
    "compile:tsc": "tsc -watch -p ./",
    "build:tsc": "tsc -p ./",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "tslint -t verbose './src/**/*'",
    "webpack": "webpack --mode development && chmod +x dist/*",
    "webpack-dev": "webpack --mode development --watch"
  },
  "license": "MIT"
}
